<h1> Create an article </h1>


<%= form_with model:  @article do | form | %>
    <div>
        <%= form.label :title %>
        <br/>
        <%= form.text_field :title %>
    </div>
<br/>
       <div>
        <%= form.label :description %>
        <br/>
        <%= form.text_area :description %>
    </div>
    <br/>
    <%= form.submit %>
<% end %>